\nh1, h2, h3, h4, h5, p {\n  margin: 0;\n  padding: 0;\n  font-family: 'Moderat';\n}\n\np {\n  font-size: 14px;\n  line-height: auto;\n  letter-spacing: 0.08em;\n}\n\ncanvas {\n  width: 100vw;\n  height: 100vh;\n  outline: none;\n  position: fixed;\n  top: 0;\n  left: 0;\n}\n\n.container {\n  position: absolute;\n  top: 0;\n  left: 0;\n  height: 100vh;\n  width: 100vw;\n}\n\n._intro {\n  // display: none;\n  position: absolute;\n  width: 100%;\n  height: 100%;\n  background-color: white;\n  z-index: 200;\n  &.hidden {\n    animation: fade-out 1s cubic-bezier(.5, 0, .5, 1) both;\n    animation-delay: calc(3ms * var(--char-index) + 1000ms );\n  }\n  &_first, &_second {\n    position: absolute;\n    display: flex;\n    flex-direction: column;\n    top: 50%;\n    left: 50%;\n    transform: translateX(-50%) translateY(-50%);\n  }\n  &_first {\n    width: 35vw;\n    align-items: flex-end;\n    .splitting .char {\n      animation: slide-in 2s cubic-bezier(.5, 0, .5, 1) both;\n      animation-delay: calc(3ms * var(--char-index) + 1000ms );\n    }\n    div {\n      display: flex;\n      align-self: flex-end;\n      p {\n        width: fit-content;\n      }\n    }\n    &-citation {\n      font-family: 'Silk';\n      font-size: 120px;\n      line-height: 90px;\n      padding-right: 10px;\n      &.splitting .char {\n        animation: slide-in-title 2s cubic-bezier(.5, 0, .5, 1) both;\n        animation-delay: calc(3ms * var(--char-index) + 1000ms );\n      }\n    }\n    &.hidden {\n      .splitting .char {\n        animation: fade-out 1s cubic-bezier(.5, 0, .5, 1) both;\n        animation-delay: calc(1ms * var(--char-index) + 300ms );\n      }\n      &-citation {\n        &.splitting .char {\n          animation: fade-out 1s cubic-bezier(.5, 0, .5, 1) both;\n          animation-delay: calc(1ms * var(--char-index) + 300ms );\n        }\n      }\n    }\n    &-name {\n      font-size: 10px;\n    }\n  }\n  &_second {\n    width: 40vw;\n    text-align: center;\n    &-title {\n      font-family: 'Silk';\n      font-size: 80px;\n      letter-spacing: 0.28em;\n    }\n    p {\n      &:nth-child(1) {\n        margin-bottom: calc(50vh - 144px);\n      }\n      &:nth-child(3) {\n        margin-top: calc(50vh - 144px);\n      }\n      &.splitting .char {\n        animation: slide-in-title 2s cubic-bezier(.5, 0, .5, 1) both;\n        animation-delay: calc(100ms * var(--char-index) + 1000ms );\n      }\n    }\n    &.hidden {\n      p {\n        &.splitting .char {\n          animation: none;\n          opacity: 0;\n        }\n      }\n    }\n  }\n}\n\n@keyframes slide-in {\n  from {\n    transform: translateY(1.5em);\n    opacity: 0;\n  }\n}\n\n@keyframes slide-in-title {\n  from {\n    transform: translateY(0.5em);\n    opacity: 0;\n  }\n}\n\n@keyframes fade-out {\n  to {\n    opacity: 0;\n  }\n}"},"lineCount":null}},"error":null,"hash":"4abe4d39ac402ed5ab06dc8ec4bcb4c2","cacheData":{"env":{}}}