enerated":{"line":63,"column":0},"source":"js/Components/Timer.js","original":{"line":41,"column":4}},{"name":"startTimer","generated":{"line":63,"column":11},"source":"js/Components/Timer.js","original":{"line":41,"column":9}},{"generated":{"line":63,"column":21},"source":"js/Components/Timer.js","original":{"line":41,"column":4}},{"generated":{"line":64,"column":0},"source":"js/Components/Timer.js","original":{"line":42,"column":3}}],"sources":{"js/Components/Timer.js":"import '../../css/timer.scss'\nexport default class Timer {\n  constructor() {\n     this.interval = 2000\n     this.time = (12 * 24 * 60 * 60 * 1000) + (12 * 60 * 60 * 1000)\n     this.endtime = '30 Dec 2020 00:12:00 GMT'\n     this.timer = null\n     this.$timer = document.querySelector('.timer')\n     this.startTimer()\n    //  this.events()\n   }\n\n  startTimer() {\n    this.timer = setInterval(() => {\n      if (this.time > 0) {\n        this.time = this.time - 1000\n\n        const countMinute = 60 * 1000\n        const countHour = 60 * countMinute\n        const countDay = 24 * countHour\n\n        const total = Date.parse(this.endtime) - Date.parse(new Date())\n        const minutes = Math.floor( (total/countMinute) % 60)\n        const hours = Math.floor( (total/countHour) % 24)\n        const days = Math.floor( total/countDay)\n\n        this.$timer.innerHTML = `d${days} h${hours} m${minutes}`\n      } else {\n        this.time = 0\n      }\n    }, this.interval)\n  }\n\n  events() {\n    window.addEventListener('click', () => this.updateTimer())\n  }\n\n  updateTimer() {\n    this.interval = this.interval/2\n    clearInterval(this.timer)\n    this.startTimer()\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"af77bba21ab86b04703489fa8811bc43","cacheData":{"env":{}}}